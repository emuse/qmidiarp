#ifndef CONFIG_H
#define CONFIG_H

/* application name */
#cmakedefine APP_NAME "@APP_NAME@"

/* build the standalone application */
#cmakedefine BUILD_LV2 @BUILD_LV2@

/* build the standalone application */
#cmakedefine BUILD_LV2_UI @BUILD_LV2_UI@

/* build with ALSA backend */
#cmakedefine HAVE_ALSA @HAVE_ALSA@

/* build with JACK backend */
#cmakedefine HAVE_JACK @HAVE_JACK@

/* build with NSM support */
#cmakedefine NSM @NSM@

/* Define to the full name of this package. */
#cmakedefine PACKAGE_NAME "@PACKAGE_NAME@"

/* Define to the full name of this package. */
#cmakedefine PACKAGE "@PACKAGE_NAME@"

/* Define to the full name and version of this package. */
#cmakedefine PACKAGE_STRING "@PACKAGE_STRING@"

/* Define to the one symbol short name of this package. */
#cmakedefine PACKAGE_TARNAME "@PACKAGE_TARNAME@"

/* Define to the home page for this package. */
#cmakedefine PACKAGE_URL "@PACKAGE_URL@"

/* Define to the version of this package. */
#cmakedefine PACKAGE_VERSION "@PACKAGE_VERSION@"

/* Version number of package */
#cmakedefine VERSION "@PACKAGE_VERSION@"

#endif
