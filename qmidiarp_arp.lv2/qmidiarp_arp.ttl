        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 127 ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "RANDOM_LEN" ;
